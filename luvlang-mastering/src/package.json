{
  "name": "luvlang-mastering",
  "version": "3.0.0",
  "description": "Professional audio mastering platform with AI-powered features",
  "main": "js/index.js",
  "type": "module",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon api/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --reporter=junit --outputFile=test-results.xml",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "build": "npm run lint",
    "precommit": "npm run lint && npm test",
    "docs": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "audio",
    "mastering",
    "lufs",
    "eq",
    "compression",
    "limiter",
    "podcast",
    "music",
    "web-audio",
    "ai"
  ],
  "author": "LuvLang",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.95.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^1.4.5-lts.1",
    "stripe": "^14.25.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luvlang/luvlang-mastering"
  },
  "bugs": {
    "url": "https://github.com/luvlang/luvlang-mastering/issues"
  },
  "homepage": "https://luvlang.com"
}
