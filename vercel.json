{
  "buildCommand": "cd vercel-deploy && npm run build",
  "devCommand": null,
  "installCommand": "cd vercel-deploy && npm install",
  "outputDirectory": "vercel-deploy",
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/landing.html"
    },
    {
      "source": "/app",
      "destination": "/luvlang_LEGENDARY_COMPLETE.html"
    },
    {
      "source": "/studio",
      "destination": "/luvlang_LEGENDARY_COMPLETE.html"
    },
    {
      "source": "/success",
      "destination": "/luvlang_LEGENDARY_COMPLETE.html"
    },
    {
      "source": "/cancel",
      "destination": "/luvlang_LEGENDARY_COMPLETE.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    }
  ]
}
